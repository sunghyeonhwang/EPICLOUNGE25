.member_body {background:#000; padding:0 0 150px}

#member_top {position:relative; width:100%; height:60px; line-height:60px; text-align:center; background:#1d1d20; transition: all 0.75s ease 0s; z-index:10}
#member_top .top_btn {position:absolute; top:10px; left:0px; height:40px; line-height:40px; display:inline-block;  }
#member_top .top_btn a {font-family:"Brutal"; font-size:11px; color:#fff; letter-spacing:1px; padding:0 18px 0 0;}
#member_top .top_btn a i.top_icon {position:relative; width:40px; height:40px; background:url('../images/menu_arrow_up.svg') no-repeat center center; background-size:cover; display:inline-block; vertical-align:middle; margin:0 8px 0 0}
#member_top .member_header_seq_box {text-align:left;}
#member_top .member_header_seq_box ul {padding:0 0 0 40px}
#member_top .member_header_seq_box ul li {display:inline-block;  padding:0 20px}
#member_top .member_header_seq_box ul li a {font-size:16px; color:#fff; transition: all 0.75s ease 0s; font-weight:600; letter-spacing:-0.025em;   font-family: 'InterTight';}
#member_top.header_2 .member_header_seq_box ul li {padding:0px}
#member_top.header_2 .member_header_seq_box ul li a {position:relative; padding:0 35px;} 
#member_top.header_2 .member_header_seq_box ul li a:before {width: 1px; height: 15px; display: block; content: "";  background: #fff;  opacity: 0.2;  position: absolute;  left: 0px;   top: 25px;}
#member_top.header_2 .member_header_seq_box ul li:first-child a:before {display:none}
#member_top .member_header_seq_box ul li.active span {color:#00AAF5}

#member_top .confirm_btn {position:absolute; top:10px; right:0px; display:inline-block;  line-height:1em}
#member_top .confirm_btn a {height:40px; line-height:40px;  font-weight:bold; border-radius:6px; }
#member_top .confirm_btn a.btn_1 {background: #2D42DD; padding:0 50px; margin:0 5px 0 0; background: linear-gradient(90deg,rgba(45, 66, 221, 1) 0%, rgba(175, 71, 246, 1) 100%); color:#fff; }
#member_top .confirm_btn a.btn_2 {color:#AF47F6; padding:0 25px; border:1px solid #AF47F6;}



h3.h3 {font-size:36px;  line-height:1.25em; color:#0191F1; font-weight:bold; margin:30px 0 15px}
h4.h4 {font-size:24px; line-height:1.25em; color:#000; font-weight:bold; margin:20px 0 15px 0px}

h4.h4 + .bu {padding:0 0 0 15px} 
.bu {padding:10px 0 0 15px}
.bu li {font-size:20px; color:#333; padding:2px 0 2px 15px; position:relative}
.bu li:before {position:absolute; top:16px; left:0px; width:2px; height:2px; display:block; background:#333; border-radius:2px}
.bu li:after {display:block; content:"";}

.bu.type2 li  {color:#000000; font-size:18px; padding:2px 0 2px 15px; word-break:keep-all; line-height:1.25em; }
.bu.type2 li:before {top:12px;    background: #A1A1A1;    left: 5px; background:#A1A1A1; }


@media all and (min-width:640px){
.bu.right_bu {width:100%; text-align:right;}
.bu.right_bu li {width:100%;}
.bu.right_bu li:before {position:Relative; top:auto; left:auto; display:inline-block; vertical-align:top; margin:10px 8px 0 0}
}

