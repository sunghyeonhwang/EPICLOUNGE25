#top_visual .top_vi_text img {height:245px}



.ipin_sel {display:flex; margin:10px 0 20px; justify-content: space-between;}
.ipin_sel .ipin_sel_box {position:relative; width:calc(50% - 16px); display:inline-block; background:#E0E0E0; }
.ipin_sel .ipin_sel_box a {width:100%; padding:10px 90px; box-sizing:border-box; display:inline-block; font-size:0px}
.ipin_sel .ipin_sel_box .icon {position:relative; width:98px; height:98px; display:inline-block; vertical-align:top;}
.ipin_sel .ipin_sel_box .text_box {position:relative; width:calc(100% - 115px); vertical-align:top; margin:5px 0 0 15px; display:inline-block}
.ipin_sel .ipin_sel_box .text_box .title {font-size:24px; color:#333; width:100%; display:inline-block; font-weight:bold}
.ipin_sel .ipin_sel_box .text_box .text {font-size:16px; color:#333; width:100%; display:inline-block;}


.ipin_sel .ipin_sel_box.phone_box .icon {background:url('../images/veri_icon1.svg') no-repeat center center; background-size:cover;}
.ipin_sel .ipin_sel_box.ipin_box .icon {background:url('../images/veri_icon2.svg') no-repeat center center;  background-size:cover;}


.team_add_box1 {position:relative; padding:40px 100px; width:100%; display:inline-block; background:#1E1E20; vertical-align:top;}

.ticket_list_box {position:relative; padding:40px 100px; width:100%; display:inline-block; background:#1E1E20; vertical-align:top;}
.ticket_list_box h4 {font-size:46px; padding-left:10px; display: inline-block; text-align: left;  font-family: 'InterTight'; background: #2D42DD; background: linear-gradient(90deg, rgba(45, 66, 221, 1) 0%, rgba(175, 71, 246, 1) 100%); background-clip: text;  -webkit-background-clip: text; color: transparent; font-weight: bold;}
.ticket_list_box ul.ticket_ul {width:100%; display:flex; justify-content: space-between; vertical-align:top;}
.ticket_list_box ul.ticket_ul li.ticket_type1 {width:calc(33.33% - 5px); float:left; display:inline-block; vertical-align:top; }
.ticket_list_box input {display:none}
.ticket_list_box label {position:relative; border:1px solid #A3A3A3; border-radius:10px; box-sizing:border-box;}
.ticket_list_box label .ticket_title { width:100%; display:inline-block; vertical-align:top; padding:30px 35px 10px; font-size:42px; font-weight:bold; color:#A3A3A3; }
.ticket_list_box label .price_box {padding:0 30px; width:100%;}
.ticket_list_box label .price_box .text_1 {position:Relative; display:inline-block; padding-left:5px;  font-size:25px;   text-decoration: line-through; opacity:0.5; color:#A3A3A3;  font-weight:200; line-height:1em;   }
.ticket_list_box label .price_box .text_2 {display:inline-block; width:100%; vertical-align:top; font-size:55px; color:#A3A3A3; line-height:1em;  font-weight:600}

.ticket_list_box label .info_box {padding:0 30px 30px;   padding-top:15px;  width:100%;}
.ticket_list_box label .info_box .text_1 {font-size:16px; color:#A3A3A3; font-weight:bold;}
.ticket_list_box label .info_box ul {position:relative; width:100%; padding-top:5px;  display:inline-block;}
.ticket_list_box label .info_box ul li {position:relative; padding:1px 0 1px 20px; font-weight:200; display:block; color:#A3A3A3;}
.ticket_list_box label .info_box ul li:before {position:absolute; top:12px; left:10px;  width:2px; height:2px; background:#A3A3A3; display:block; content:""; }


.ticket_list_box input[type="radio"]:checked + label {background: #2D42DD; border:none; background: linear-gradient(90deg,rgba(45, 66, 221, 1) 0%, rgba(175, 71, 246, 1) 100%);}

.ticket_list_box input[type="radio"]:checked + label .ticket_title { color:#fff}
.ticket_list_box input[type="radio"]:checked + label .price_box .text_1 {color:#fff}
.ticket_list_box input[type="radio"]:checked + label .price_box .text_2 {color:#fff}
.ticket_list_box input[type="radio"]:checked + label .info_box .text_1 { color:#fff}
.ticket_list_box input[type="radio"]:checked + label .info_box ul li { color:#fff;}
.ticket_list_box input[type="radio"]:checked + label .info_box ul li:before { background:#fff; }

.ticket_list_box ul li.ticket_type2 input[type="radio"]:checked + label .ticket_title { color:#fff}
.ticket_list_box ul li.ticket_type2 input[type="radio"]:checked + label .price_box .text_1 {color:#fff}
.ticket_list_box ul li.ticket_type2 input[type="radio"]:checked + label .price_box .text_2 {color:#fff}


ul.yellow_bu {width:100%; display:inline-block; margin-top:10px;}
ul.yellow_bu li {position:relative; color:#FFD31E; padding:2px 0 2px 35px; font-size:17px;}
ul.yellow_bu li:before {position:absolute; top:5px; left:10px; width:20px; height:20px; display:block; content:""; background:url('../images/alert_yellow.png') no-repeat left center;  background-size:20px;}



.member_cont_box {padding:40px 100px; background:#fff;}


.team_add_box1 .lr_box {width:100%; display:inline-block; vertical-align:top;}

.team_add_box1 .lr_box h4.h4 {color:#fff; font-size:20px; margin:20px 0 5px;}
.team_add_box1 .lr_box h3.h3 {margin:10px 0 0;}
.team_add_box1 .lr_box ul li {color:#fff; font-weight:200; font-size:17px; padding:2px 0 2px 10px}
.team_add_box1 .lr_box ul li:before {background:#fff; top:10px; left:0px;}
.team_add_box1 .left_box {width:50%; padding-right:10px; float:left;}
.team_add_box1 .right_box {width:50%; padding-left:10px; float:left;}


.track_radio_box {}
.track_radio_box ul {position:relative; width:100%; display:inline-block;}
.track_radio_box ul li {display:inline-block; margin:0 40px 0 30px;}
.track_radio_box ul li.fullbooking {opacity:0.5}
.track_radio_box ul li.fullbooking before {background:#ccc; border:2px solid #ccc}
.track_radio_box ul li input {display:none}
.track_radio_box ul li label {position:relative; font-size:34px; padding:5px 0 5px 50px; color:#fff; font-weight:600}
.track_radio_box ul li label:before {position:absolute; left:0px; top:10px; display:block; border:2px solid #fff; content:""; width:36px; height:36px}
.track_radio_box input[type="radio"]:checked + label:before {background:#00AAF5}

 

.member_form_box {font-size:0px; width:calc(100% + 20px); margin:0 0 0 -10px; }
.member_form_box .member_form_list {width:calc(50% - 20px); display:inline-block; vertical-align: top; margin:0 10px 25px}
.member_form_box label {font-size:24px; color:#000; margin:0 0 5px 0}
.member_form_box input[type="text"], 
.member_form_box select { height:59px; width:100%; background:#E0E0E0; border:none; font-family: 'Freesentation';  color:#757575; font-size:24px; padding:0 20px; box-sizing:border-box;}

.member_form_box select {appearance:none; background:url('../images/arrow_down.png') no-repeat right 18px center #E0E0E0; background-size:16px auto}
.member_form_box select:focus {background:url('../images/arrow_up.png') no-repeat right 18px center #E0E0E0; background-size:16px auto}
.member_form_box input[type="text"]:placeholder {color:#6A6A6A;}
.member_form_box select.placeholder {color: #6A6A6A !important;}
.member_form_box .file_box {position:relative; width:100%; display:inline-block}
.member_form_box .file_box .file_upload {display:none;}
.member_form_box .file_box .file_name {position:relative; width:calc(100% - 105px); height:59px;  background:#E0E0E0; display:inline-block; float:left }
.member_form_box .file_box .upload-button {position:relative; width:93px; height:59px; background:#00AAF5; color:#000; font-size:24px; float:right;}
.member_form_box .coupon_form {position:relative}
.member_form_box .coupon_form .coupon_btn {position:absolute; top:0px; right:-103px; height:59px; line-height:59px; width:93px; font-size:24px; color:#010101; font-weight:bold; background:#FFC228; text-align:center;}
.member_form_box .text {font-size:16px; color:#fff; margin-top:10px}


.member_agree_box {font-size:0px; width:calc(100% + 20px); margin:0 0 0 -10px; }
.member_agree_box .member_agree_list {width:calc(50% - 20px); display:inline-block; vertical-align: top; margin:0 10px 25px}
.member_agree_box .member_agree_list .text {font-size:24px; color:#fff;}
.member_agree_box .member_agree_list .agree_box {position:Relative; width:100%;  display:inline-block; vertical-align:top; margin-top:5px}
.member_agree_box .member_agree_list .agree_box .agree_wrap {background:#E0E0E0; height:200px; overflow-y:auto; font-size:18px; color:#6A6A6A; padding:20px 30px;}
.member_agree_box .member_agree_list .agree_box .agree_wrap::-webkit-scrollbar {
    width: 5px;  
}
.member_agree_box .member_agree_list .agree_box .agree_wrap::-webkit-scrollbar-thumb {
    background: rgba(255,255,255,0.4); /* 스크롤바 색상 */
    border-radius: 10px; /* 스크롤바 둥근 테두리 */
}
.member_agree_box .member_agree_list .agree_box .agree_wrap::-webkit-scrollbar-track {
    background: #26262F;  /*스크롤바 뒷 배경 색상*/
}

.member_agree_box .member_agree_list .agree_ch {text-align:right; width:100%; display:inline-block; vertical-align:top; margin-top:10px}
.member_agree_box .member_agree_list .agree_ch input {margin:0 5px 0 0}
.member_agree_box .member_agree_list .agree_ch label {font-size:16px; color:#8e8e8e;}

.agree_all {text-align:center; width:100%; display:inline-block; vertical-align:top; margin:50px 0 0 0}
.agree_all label {font-size:16px; color:#fff; }
.agree_all input {margin:0 5px 0 0}


.modal_on {overflow:hidden;}
.modal_on:before {position:fixed; top:0px; left:0px; width:100vw; height:100vh; background:rgba(0,0,0,0.6); display:block; z-index:100}
